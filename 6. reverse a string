class Solution {
    static String revStr(String s) {
        // code here
          char[] arr = s.toCharArray();
        int i = 0, j = arr.length - 1;
        while (i < j) {
            char t = arr[i];
            arr[i] = arr[j];
            arr[j] = t;
            i++;
            j--;
        }
        return new String(arr);
    }
}
