class Solution {
    public void reverse(int start,int end,int[] nums)
    {
        while(start<end){
            int temp=nums[start];
            nums[start]=nums[end];
            nums[end]=temp;
            start++;
            end--;
        }
    }
    public void rotate(int[] nums, int k) {
        int size=nums.length;
        k=k%size;
        int calc=size-k;
        reverse(0,calc-1,nums);
        reverse(calc,size-1,nums);
        reverse(0,size-1,nums);
    }


        
    }
